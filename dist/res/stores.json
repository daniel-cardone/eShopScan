{
  "Amazon": {
    "website": "amazon.com",
    "img": "#landingImage",
    "name": "#productTitle",
    "price": ".a-offscreen",
    "form": {
      "general": {
        "container": "div[id^=variation_]",
        "parents": 0,
        "label": {
          "func": ["querySelector", "textContent", "replace", "trim"],
          "args": [["label"], null, [":", ""], []],
          "labelsToIgnore": [
            {
              "type": "label-has-text",
              "rule": "Color"
            }
          ]
        },
        "options": {
          "query": "option:not(:first-child), button",
          "func": ["textContent", "trim"],
          "args": [null, []]
        }
      },
      "extra": {
        "Color": {
          "query": "div[id^=variation_color] img",
          "func": ["getAttribute"],
          "args": [["alt"]]
        }
      }
    }
  },
  "Nike": {
    "website": "nike.com",
    "img": "picture img",
    "name": "#pdp_product_title",
    "price": ".product-price",
    "form": {
      "general": {
        "container": "fieldset:not(.colorway-images)",
        "parents": 0,
        "label": {
          "func": ["querySelector", "textContent", "split", 1],
          "args": [["span"], null, [" "], null],
          "labelsToIgnore": []
        },
        "options": {
          "query": "label",
          "func": ["textContent"],
          "args": [null]
        }
      },
      "extra": {
        "Color": {
          "query": "fieldset.colorway-images img",
          "func": ["getAttribute"],
          "args": [["alt"]]
        }
      }
    }
  },
  "Dick's Sporting Goods": {
    "website": "dickssportinggoods.com",
    "img": "[data-cy*=product-image]",
    "name": "h1.title",
    "price": ".product-price",
    "form": {
      "general": {
        "container": "div[id^=attr_]",
        "parents": 0,
        "label": {
          "func": ["getAttribute", "split", 1],
          "args": [["id"], ["_"], null],
          "labelsToIgnore": [
            {
              "type": "label-has-text",
              "rule": "Color"
            }
          ]
        },
        "options": {
          "query": "button.btn-attr-select",
          "func": ["textContent"],
          "args": [null]
        }
      },
      "extra": {
        "Color": {
          "query": "button.swatch-color",
          "func": ["querySelector", "getAttribute"],
          "args": [["img"], ["alt"]]
        }
      }
    }
  },
  "Olympia Sports": {
    "website": "olympiasports.net",
    "img": "#aaaaaaaaaaaaaaaaaaaaaaaa",
    "name": "#aaaaaaaaaaaaaaa",
    "price": "#aaaaaaaaaaaa"
  },
  "Big Lots": {
    "website": "biglots.com",
    "img": "[id^=resolve_]",
    "name": ".product-name h1",
    "price": ".regular-price",
    "form": {
      "general": {
        "container": "div[id^=variantType-]",
        "parents": 0,
        "label": {
          "func": ["querySelector", "textContent", "split", 0],
          "args": [[".type--label"], null, [":"], null],
          "labelsToIgnore": [
            {
              "type": "label-has-text",
              "rule": "color"
            }
          ]
        },
        "options": {
          "query": "li",
          "func": ["textContent"],
          "args": [null]
        }
      },
      "extra": {
        "Color": {
          "query": "#variantType-color li",
          "func": ["dataset", "id", "replaceAll"],
          "args": [null, null, ["&amp;", "&"]]
        }
      }
    }
  },
  "Walgreens": {
    "website": "walgreens.com",
    "img": "#productImg",
    "name": "#productTitle",
    "price": "#regular-price-info",
    "form": {
      "general": {
        "container": "[id^=prod-][id$=-swatch]",
        "parents": 0,
        "label": {
          "func": ["getAttribute", "split", 1],
          "args": [["id"], ["-"], null],
          "labelsToIgnore": [
            {
              "type": "label-has-text",
              "rule": "color"
            }
          ]
        },
        "options": {
          "query": "li",
          "func": ["textContent"],
          "args": [null]
        }
      },
      "extra": {
        "Color": {
          "query": "#prod-color-swatch li a",
          "func": ["getAttribute"],
          "args": [["title"]]
        }
      }
    }
  },
  "Famous Footwear": {
    "website": "famousfootwear.com",
    "img": ".product-image img",
    "name": ".product-info__product-name",
    "price": ".bfx-price",
    "form": {
      "general": {
        "container": "div.VariantTileContainer",
        "parents": 1,
        "label": {
          "func": ["querySelector", "textContent", "replace"],
          "args": [["label"], null, [":", ""]],
          "labelsToIgnore": [
            {
              "type": "parent-matches-selector",
              "rule": "[style*='none']"
            },
            {
              "type": "parent-has-text",
              "rule": "Size"
            }
          ]
        },
        "options": {
          "query": ".VariantTile",
          "func": ["textContent"],
          "args": [null]
        }
      },
      "extra": {
        "Color": {
          "query": "img.product-color-image--thumbnail",
          "func": ["alt"],
          "args": [null]
        },
        "Age Group": {
          "query": ".VariantLabel ~ span ~ .VariantTileContainer:nth-last-of-type(3) a",
          "func": ["textContent"],
          "args": [null]
        },
        "Size": {
          "query": ".VariantLabel ~ span ~ .VariantTileContainer:nth-last-of-type(2) a",
          "func": ["textContent"],
          "args": [null]
        },
        "Width": {
          "query": "label.VariantLabel:last-of-type ~ .VariantTileContainer a",
          "func": ["textContent"],
          "args": [null]
        }
      }
    }
  },
  "Michaels": {
    "website": "michaels.com",
    "img": "img[id*=zoom]",
    "name": ".product-name",
    "price": ".product-sales-price",
    "form": {
      "general": {
        "container": "div.product-variations li.attribute",
        "parents": 0,
        "label": {
          "func": ["querySelector", "textContent", "split", 0],
          "args": [["h3"], null, [":"], null],
          "labelsToIgnore": [
            {
              "type": "label-has-text",
              "rule": "Color"
            }
          ]
        },
        "options": {
          "query": "a",
          "func": ["textContent", "trim"],
          "args": [null, []]
        }
      },
      "extra": {
        "Color": {
          "query": "ul.Color img",
          "func": ["getAttribute"],
          "args": [["title"]]
        }
      }
    }
  }
}