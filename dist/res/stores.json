{
  "Amazon": {
    "website": "amazon.com",
    "img": "#landingImage",
    "name": "#productTitle",
    "price": ".a-offscreen",
    "stock": {
      "shipping": {
        "selector": "#availability",
        "text": "n stock",
        "default": true
      }
    }
  },
  "Nike": {
    "website": "nike.com",
    "img": "picture img",
    "name": "#pdp_product_title",
    "price": ".product-price"
  },
  "Dick's Sporting Goods": {
    "website": "dickssportinggoods.com",
    "img": "[data-cy*=product-image]",
    "name": "h1.title",
    "price": ".product-price",
    "type": "size-color-boxes",
    "form": {
      "Size": {
        "element": "button.swatch-disabled:not([class*=swatch-color])",
        "data": "textContent"
      },
      "Color": {
        "element": "button.swatch-color",
        "data": "alt"
      }
    }
  },
  "Olympia Sports": {
    "website": "olympiasports.net",
    "img": "#aaaaaaaaaaaaaaaaaaaaaaaa",
    "name": "#aaaaaaaaaaaaaaa",
    "price": "#aaaaaaaaaaaa"
  },
  "Big Lots": {
    "website": "biglots.com",
    "img": "#resolve_1",
    "name": ".product-name h1",
    "price": ".regular-price",
    "stock": ".out-of-stock-msg"
  },
  "Walgreens": {
    "website": "walgreens.com",
    "img": "#productImg",
    "name": "#productTitle",
    "price": "#regular-price-info",
    "type": "stock-text",
    "check": {
      "pickup": {
        "click": "#wag-pickup-tab",
        "selector": "[id*='Out of Stock']"
      },
      "shipping": {
        "click": "#wag-shipping-tab",
        "selector": ".wag-shipping-inner-content",
        "default": true
      }
    }
  },
  "Famous Footwear": {
    "website": "famousfootwear.com",
    "img": ".product-image img",
    "name": ".product-info__product-name",
    "price": ".bfx-price",
    "stock": {}
  },
  "Michaels": {
    "website": "michaels.com",
    "img": "img[id*=zoom]",
    "name": ".product-name",
    "price": ".product-sales-price",
    "stock": {}
  }
}