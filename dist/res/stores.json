{
  "Amazon": {
    "website": "amazon.com",
    "img": "#landingImage",
    "name": "#productTitle",
    "price": ".a-offscreen",
    "form": {
      "general": {
        "container": "div[id^=variation_]",
        "parents": 0,
        "label": {
          "func": ["querySelector", "textContent", "replace", "trim"],
          "args": [["label"], null, [":", ""], []],
          "labelsToIgnore": [
            {
              "type": "label-has-text",
              "rule": "Color"
            }
          ]
        },
        "options": {
          "query": "option:not(:first-child), button",
          "func": ["textContent", "trim"],
          "args": [null, []]
        }
      },
      "extra": {
        "Color": {
          "element": "div[id^=variation_color] img",
          "data": ["getAttribute"],
          "args": [["alt"]]
        }
      }
    }
  },
  "Nike": {
    "website": "nike.com",
    "img": "picture img",
    "name": "#pdp_product_title",
    "price": ".product-price",
    "form": {
      "general": {
        "container": "fieldset:not(.colorway-images)",
        "parents": 0,
        "label": {
          "func": ["querySelector", "textContent", "split", 1],
          "args": [["span"], null, [" "], null],
          "labelsToIgnore": []
        },
        "options": {
          "query": "label",
          "func": ["textContent"],
          "args": [null]
        }
      },
      "extra": {
        "Color": {
          "element": "fieldset.colorway-images img",
          "data": ["getAttribute"],
          "args": [["alt"]]
        }
      }
    }
  },
  "Dick's Sporting Goods": {
    "website": "dickssportinggoods.com",
    "img": "[data-cy*=product-image]",
    "name": "h1.title",
    "price": ".product-price",
    "form": {
      "general": {
        "container": "div[id^=attr_]",
        "parents": 0,
        "label": {
          "func": ["getAttribute", "split", 1],
          "args": [["id"], ["_"], null],
          "labelsToIgnore": [
            {
              "type": "label-has-text",
              "rule": "Color"
            }
          ]
        },
        "options": {
          "query": "button.btn-attr-select",
          "func": ["textContent"],
          "args": [null]
        }
      },
      "extra": {
        "Color": {
          "element": "button.swatch-color",
          "data": ["querySelector", "getAttribute"],
          "args": [["img"], ["alt"]]
        }
      }
    }
  },
  "Olympia Sports": {
    "website": "olympiasports.net",
    "img": "#aaaaaaaaaaaaaaaaaaaaaaaa",
    "name": "#aaaaaaaaaaaaaaa",
    "price": "#aaaaaaaaaaaa"
  },
  "Big Lots": {
    "website": "biglots.com",
    "img": "#resolve_1",
    "name": ".product-name h1",
    "price": ".regular-price",
    "stock": ".out-of-stock-msg"
  },
  "Walgreens": {
    "website": "walgreens.com",
    "img": "#productImg",
    "name": "#productTitle",
    "price": "#regular-price-info",
    "check": {
      "pickup": {
        "click": "#wag-pickup-tab",
        "selector": "[id*='Out of Stock']"
      },
      "shipping": {
        "click": "#wag-shipping-tab",
        "selector": ".wag-shipping-inner-content",
        "default": true
      }
    }
  },
  "Famous Footwear": {
    "website": "famousfootwear.com",
    "img": ".product-image img",
    "name": ".product-info__product-name",
    "price": ".bfx-price",
    "form": {
      "general": {
        "container": "div.VariantTileContainer",
        "parents": 1,
        "label": {
          "func": ["querySelector", "textContent", "replace"],
          "args": [["label"], null, [":", ""]],
          "labelsToIgnore": [
            {
              "type": "parent-matches-selector",
              "rule": "[style*='none']"
            },
            {
              "type": "parent-has-text",
              "rule": "Size"
            }
          ]
        },
        "options": {
          "query": ".VariantTile",
          "func": ["textContent"],
          "args": [null]
        }
      },
      "extra": {
        "Color": {
          "element": "img.product-color-image--thumbnail",
          "data": ["alt"],
          "args": [null]
        },
        "Size": {
          "element": ".VariantLabel ~ span ~ .VariantTileContainer:first-of-type a",
          "data": ["textContent"],
          "args": [null]
        },
        "Width": {
          "element": "label.VariantLabel:nth-of-type(2) ~ .VariantTileContainer a",
          "data": ["textContent"],
          "args": [null]
        }
      }
    }
  },
  "Michaels": {
    "website": "michaels.com",
    "img": "img[id*=zoom]",
    "name": ".product-name",
    "price": ".product-sales-price",
    "stock": {}
  }
}